#include<stdio.h>
int main()
{   
int sum2=0,sum1=0,n=100;
    for(int i=1;i<=n;i++)
    {
        sum2+=i;
        sum1+=i*i;
    }
printf("%d\n",sum2*sum2-sum1);
    return 0;
}
